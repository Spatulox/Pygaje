function fibonacci(n) {
    if (n <= 1) {
        return n;
    } else {
        return fibonacci(n-1) + fibonacci(n-2);
    }
}

function printArray(arr) {
    for (i = 0; i < 5; i = i + 1) {
        print arr[i];
    }
}

numbers = [0, 1, 2, 3, 4];
print "Original array:";
printArray(numbers);

i = 0;
while (i < 5) {
    numbers[i] = fibonacci(i);
    i = i + 1;
}

print("Array after Fibonacci:");
printArray(numbers);

sum = 0;
for (i = 0; i < 5; i = i + 1) {
    sum = sum + numbers[i];
    if (sum > 10) {
        print "Sum exceeded 10";
        break;
    }
}

print("Final sum:");
print(sum);

if (sum % 2 == 0) {
    print "Sum is even";
} else {
    print "Sum is odd";
}

exit;
